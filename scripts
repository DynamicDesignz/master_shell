#!/bin/bash

cd /etc/apache2/sites-available/

echo -n "Virtual Host Name: "
read example
sudo cp 000-default.conf $example.com.conf
#!/bin/bash

echo -n "Deny: ~$ "
read deny
sudo echo "ALL: $deny" >> /etc/hosts.deny
echo "DENIED $deny"
echo ""
cat /etc/hosts.deny

#!/bin/bash

echo -n "Enter DenyHost IP: "
read ip

echo $ip >> /etc/hosts.deny

#!/bin/bash

echo -n "Dig Domain A: "
read domain
dig $domain –t A

#!/bin/bash

echo -n "Dig Domain MX: "
read domain
dig $domain –t MX
